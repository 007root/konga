{
  "name": "kongadmin",
  "version": "0.4.0",
  "description": "Kong admin GUI",
  "keywords": [
    "sails.js",
    "kong",
    "admin",
    "API",
    "JWT",
    "JSON Web Token",
    "Passport.js",
    "frontend",
    "angular",
    "slush",
    "slush-angular"
  ],
  "main": "",
  "dependencies": {
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "concurrently": "^3.1.0"
  },
  "scripts": {
    "install": "cd frontend && npm install --production && cd ../backend && npm install --production",
    "start": "node ./bin/konga play",
    "production": "concurrently \"(cd backend && node app.js --prod)\" \"(cd frontend && node server.js)\"",
    "test": "cd backend && mocha"
  },
  "bin": {
    "konga": "./bin/konga.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pantsel/konga.git"
  },
